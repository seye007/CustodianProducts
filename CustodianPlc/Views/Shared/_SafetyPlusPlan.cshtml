function validateRequiredField($input) {
    if (!$input.val()) {
        var errorMessage = $input.data('val-required') || $input.data('val-imageformatvalidation') || $input.data('val-ImageSizeValidation');
        //alert(errorMessage);
        Swal.fire({
            icon: 'error',
            confirmButtonText: 'OK',
            title: 'Required!',
            text: errorMessage,
        });
        return false;
    }
    return true;
}


