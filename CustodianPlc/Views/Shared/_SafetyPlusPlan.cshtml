$(document).ready(function() {
    $('#units').change(function() {
        var unit = $('#units').val();
        $.ajax({
            url: '/Sales/Safety-Plus-Plan/CalculatePremium?unit=' + unit,
            type: 'GET',
            success: function(result) {
                $('#showcase-premium').val('NGN ' + result.premium.toFixed(2).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ","));
